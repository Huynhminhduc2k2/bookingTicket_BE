    "start:windows": "tsc && set NODE_ENV=production && node dist/index.js",
    "build": "rimraf dist & tsc -p tsconfig.json",
    "format": "yarn pretty && yarn lint",
    "pretty": "prettier --write \"./**/*.{ts,tsx,js,jsx,json}\" --end-of-line lf",
    "pretty:check": "prettier --check \"./**/*.{ts,tsx,js,jsx,json}\" --end-of-line lf",
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register 'tests/**/*.ts' --timeout 10000",
    "seed:init": "md-seed init",
    "seed": "md-seed run -d"
  },
